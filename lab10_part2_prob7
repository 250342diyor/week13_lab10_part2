#include <iostream>
using namespace std;

int findLargestElement(const int* arr, int size){
    int largest = *arr;
    for(int i = 1; i < size; i++){
        if(*(arr + i) > largest)
            largest = *(arr + i);
    }
    return largest;
}

int main(){
    int n;
    cout << "Enter n: ";
    cin >> n;

    int arr[n];
    cout << "Enter elements: ";
    for(int i = 0; i < n; i++) cin >> arr[i];

    cout << "The output is " << findLargestElement(arr, n);
}
