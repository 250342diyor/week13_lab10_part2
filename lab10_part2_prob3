#include <iostream>
using namespace std;
int main() {
    srand(time(NULL));
    int array[10];
    int *ptrA=array;
    for (int i = 1; i < 10; i++) {
        array[i]=rand()%1000;
    }
    for (int i = 0; i < 10; i++) {
        cout <<"element is: "<<i+1<<'\t'<<"address is: "<<ptrA+i<<"\t"<< "value is: "<<*(ptrA+i) << endl;
    }
}
