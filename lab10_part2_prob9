#include <iostream>
using namespace std;

void factorial(int n, int* fact){
    *fact = 1;
    for(int i = 1; i <= n; i++){
        *fact *= i;
    }
}

int main(){
    int n;
    cout << "Enter the number: ";
    cin >> n;

    int result;
    factorial(n, &result);

    cout << "The output is " << result;
}
