#include <iostream>
using namespace std;

void reverseArray(int* arr, int size){
    int* left = arr;
    int* right = arr + size - 1;

    while(left < right){
        int temp = *left;
        *left = *right;
        *right = temp;
        left++;
        right--;
    }
}

int main(){
    int n;
    cout << "Enter size: ";
    cin >> n;

    int arr[n];
    cout << "Enter elements: ";
    for(int i = 0; i < n; i++) cin >> arr[i];

    reverseArray(arr, n);

    cout << "The output is: ";
    for(int i = 0; i < n; i++) cout << arr[i] << " ";
}
